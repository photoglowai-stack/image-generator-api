{
  "version": 2,
  "routes": [
    {
      "src": "/(v1|api)/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS,HEAD",
        "Access-Control-Allow-Headers": "content-type, authorization, idempotency-key, x-admin-token"
      },
      "continue": true
    },

    { "src": "/v1/jobs$", "dest": "/api/v1-jobs.mjs" },
    { "src": "/v1/jobs/(.*)", "dest": "/api/v1-jobs.mjs" },

    { "src": "/api/generate-gen4-image$", "dest": "/api/generate-gen4-image.mjs" },
    { "src": "/api/credits$", "dest": "/api/credits.mjs" }
  ]
}
